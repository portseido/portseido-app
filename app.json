{
  "expo": {
    "name": "Portseido",
    "slug": "portseido-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/portseido_logo_with_frame.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/portseido_logo_with_frame.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portseido.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/portseido_logo_with_frame.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.portseido.app"
    },
    "web": {
      "favicon": "./assets/portseido_logo_no_text.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "portseido-dev.us.auth0.com"
        }
      ]
    ]
  }
}
